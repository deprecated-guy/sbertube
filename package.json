{
  "name": "sbertube-2",
  "packageManager": "pnpm@8.9.2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lintcheck": "eslint --quiet --ext .js,.ts,.tsx,.astro .",
    "typecheck": "tsc",
    "prepare": "husky install & npx lint-staged & npx simple-git-hooks",
    "commitlint": "commitlint --edit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged & npx stylelint **/*.sccs",
      "pre-push": "npx lint-staged & npx stylelint **/*.sccs"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add . -f "
    ],
    "*.scss": [
      "npx stylelint **/*.scss"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0-rc.0",
    "@angular/common": "^17.0.0-rc.0",
    "@angular/compiler": "^17.0.0-rc.0",
    "@angular/core": "^17.0.0-rc.0",
    "@angular/forms": "^17.0.0-rc.0",
    "@angular/language-service": "^16.2.11",
    "@angular/platform-browser": "^17.0.0-rc.0",
    "@angular/platform-browser-dynamic": "^17.0.0-rc.0",
    "@angular/router": "^17.0.0-rc.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@types/ffmpeg": "^1.0.6",
    "@types/hammerjs": "^2.0.43",
    "@videogular/ngx-videogular": "^8.0.0",
    "date-fns": "^2.30.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "hammerjs": "^2.0.8",
    "lint-staged": "^15.0.2",
    "ngx-mask": "^16.3.9",
    "ngx-videogular": "^13.2.4",
    "pnpm": "^8.9.2",
    "prettier-eslint-config": "^1.0.7",
    "rxjs": "~7.8.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order-config-standard": "^0.1.3",
    "tslib": "^2.3.0",
    "videogular2": "^6.0.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0-rc.0",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^17.0.0-rc.0",
    "@angular/compiler-cli": "^17.0.0-rc.0",
    "@ffmpeg/core": "^0.12.4",
    "@ffmpeg/ffmpeg": "^0.12.7",
    "@html-eslint/eslint-plugin": "^0.20.0",
    "@html-eslint/parser": "^0.20.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "commitlint": "^17.8.0",
    "eslint": "^8.51.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "ffmpeg": "^0.0.4",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2"
  }
}
